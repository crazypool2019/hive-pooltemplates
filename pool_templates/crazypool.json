            },
            "ethminer": {
                "cuda": 1,
                "opencl": 1,
                "pass": "x",
                "port": "%URL_PORT%",
                "server": "stratum1+tcp://%URL_HOST%",
                "template": "%WAL%"
            },
            "asicminer": {
                "url": "stratum+tcp://%URL%",
                "template": "%WAL%"
            },
            "phoenixminer": {
                "url": "POOL: %URL%, WALLET: %WAL%, WORKER: %WORKER_NAME%",
